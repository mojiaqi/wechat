{
    "name": "wechat-template-message",
    "version": "1.0.0",
    "description": "微信模板消息管理系统",
    "scripts": {
      "dev": "serve .",
      "build": "mkdir -p public && cp -r *.html public/ && echo 'Build completed'"
    },
    "dependencies": {
      "serve": "^14.2.1"
    },
    "devDependencies": {},
    "engines": {
      "node": ">=14.0.0"
    },
    "version": 2,
    "builds": [
      {
        "src": "*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }